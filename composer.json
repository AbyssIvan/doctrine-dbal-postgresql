{
  "name": "opsway/doctrine-dbal-postgresql",
  "description": "Extensions for support Postgres in Doctrine DBAL & DQL",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Vronskiy",
      "email": "alvro@opsway.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "doctrine/dbal": "^3.2",
    "doctrine/orm": "^2.10"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "opsway/psr12-strict-coding-standard": "^0.7.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "symfony/cache": "^6.0",
    "doctrine/annotations": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "Opsway\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpsWay\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  }
}
